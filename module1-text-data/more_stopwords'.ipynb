     "text": [
      "\"coffee\"\n",
      "is_stop: False\n",
      "is in set: True\n",
      "\n",
      "\"and\"\n",
      "is_stop: True\n",
      "is in set: True\n",
      "\n",
      "\"tea\"\n",
      "is_stop: False\n",
      "is in set: False\n",
      "\n"
     ]
    }
   ],
   "source": [
    "import spacy\n",
    " \n",
    "nlp = spacy.load(\"en_core_web_lg\") \n",
    "\n",
    "nlp.Defaults.stop_words.add(\"coffee\")\n",
    "\n",
    "doc = nlp('coffee and tea')\n",
    "for token in doc:\n",
    "    print(f'\"{token.lemma_}\"')\n",
    "    print(f'is_stop: {token.is_stop}')\n",
    "    print(f'is in set: {token.lemma_ in nlp.Defaults.stop_words}\\n')"
   ]
  },
